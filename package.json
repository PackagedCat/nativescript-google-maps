{
	"name": "nativescript-google-maps",
	"version": "8.0.2",
	"description": "Google Maps NativeScript plugin.",
	"repository": {
		"type": "git",
		"url": "https://github.com/PackagedCat/nativescript-google-maps.git"
	},
	"scripts": {
		"clean": "git clean -f -X -d",
		"setup": "npm run clean && npm install && ts-patch install",
		"build": "tsc -b -v && cd ./src && cpy ./**/*.d.ts ../packages/ --parents",
		"lint": "eslint ./src --ext .ts"
	},
	"keywords": [
		"Nativescript",
		"Google",
		"Maps",
		"API",
		"SDK"
	],
	"author": {
		"name": "Shamsuddin Dzhamalov",
		"email": "packagedcat@outlook.com"
	},
	"bugs": {
		"url": "https://github.com/PackagedCat/nativescript-google-maps/issues"
	},
	"license": "Apache-2.0",
	"homepage": "https://github.com/PackagedCat/nativescript-google-maps",
	"devDependencies": {
		"@angular/core": "^12.2.13",
		"@nativescript/angular": "^12.2.0",
		"@nativescript/core": "^8.1.5",
		"@nativescript/types": "^8.1.1",
		"@nativescript/webpack": "^3.0.9",
		"@typescript-eslint/eslint-plugin": "^4.33.0",
		"@typescript-eslint/parser": "^4.33.0",
		"@vue/eslint-config-typescript": "^9.0.1",
		"cpy-cli": "^3.1.1",
		"eslint": "^7.32.0",
		"eslint-plugin-vue": "^8.0.3",
		"ts-patch": "^1.4.5",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"ts-node": "^9.1.1"
	}
}
