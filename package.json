{
	"name": "nativescript-google-maps",
	"version": "8.0.2",
	"description": "Google Maps NativeScript plugin.",
	"repository": {
		"type": "git",
		"url": "https://github.com/PackagedCat/nativescript-google-maps.git"
	},
	"scripts": {
		"clean": "git clean -f -X -d",
		"setup": "npm run clean && npm install && ts-patch install",
		"build": "tsc -b -v && cd ./src && cpy ./**/*.d.ts ../packages/ --parents",
		"lint": "eslint ./src --ext .ts"
	},
	"keywords": [
		"Nativescript",
		"Google",
		"Maps",
		"API",
		"SDK"
	],
	"author": {
		"name": "Shamsuddin Dzhamalov",
		"email": "packagedcat@outlook.com"
	},
	"bugs": {
		"url": "https://github.com/PackagedCat/nativescript-google-maps/issues"
	},
	"license": "Apache-2.0",
	"homepage": "https://github.com/PackagedCat/nativescript-google-maps",
	"devDependencies": {
		"@angular/core": "^13.3.6",
		"@nativescript/angular": "^13.0.3",
		"@nativescript/core": "^8.2.3",
		"@nativescript/types": "^8.2.0",
		"@nativescript/webpack": "^5.0.6",
		"@typescript-eslint/eslint-plugin": "^5.22.0",
		"@typescript-eslint/parser": "^5.22.0",
		"@vue/eslint-config-typescript": "^10.0.0",
		"cpy-cli": "^4.1.0",
		"eslint": "^8.14.0",
		"eslint-plugin-vue": "^8.7.1",
		"ts-patch": "^2.0.1",
		"typescript": "^4.6.4"
	},
	"dependencies": {
		"ts-node": "^10.7.0"
	}
}
