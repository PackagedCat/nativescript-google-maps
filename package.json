{
	"name": "nativescript-google-maps",
	"version": "7.0.0",
	"description": "Google Maps NativeScript plugin.",
	"repository": {
		"type": "git",
		"url": "https://github.com/PackagedCat/nativescript-google-maps.git"
	},
	"scripts": {
		"clean": "git clean -f -X -d",
		"setup": "npm install && ts-patch install",
		"build": "tsc -b -v && cd ./src && cpy ./**/*.d.ts ../packages/ --parents"
	},
	"keywords": [
		"Nativescript",
		"Google",
		"Maps",
		"API",
		"SDK"
	],
	"author": {
		"name": "Shamsuddin Dzhamalov",
		"email": "packagedcat@outlook.com"
	},
	"bugs": {
		"url": "https://github.com/PackagedCat/nativescript-google-maps/issues"
	},
	"license": "Apache-2.0",
	"homepage": "https://github.com/PackagedCat/nativescript-google-maps",
	"devDependencies": {
		"@angular/core": "^12.0.4",
		"@nativescript/angular": "^12.0.1",
		"@nativescript/core": "^8.0.0",
		"@nativescript/types": "^8.0.0",
		"@nativescript/webpack": "^3.0.9",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"cpy-cli": "^3.1.1",
		"eslint": "^7.22.0",
		"ts-patch": "^1.3.0",
		"typescript": "^4.0.5"
	},
	"dependencies": {
		"ts-node": "^9.1.1"
	}
}
